{
  "name": "gather",
  "displayName": "Gather",
  "description": "Adds gather functionality to the Python Extension",
  "version": "2020.9.0",
  "publisher": "ms-python",
  "author": {
    "name": "Microsoft Corporation"
  },
  "homepage": "https://github.com/Microsoft/vscode-python",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode-gather"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/vscode-python/issues"
  },
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:jupyter",
    "onNotebook:jupyter-notebook",
    "onCustomEditor:viewType"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gather.gatherCodeWebview",
        "title": "Gather code webview"
      },
      {
        "command": "gather.gatherCodeNativeNotebook",
        "title": "Gather code",
        "icon": "$(group-by-ref-type)"
      },
      {
        "command": "gather.quality",
        "title": "Gather.quality"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "gather.gatherCodeWebview",
          "title": "Gather code webview",
          "when": "false"
        },
        {
          "command": "gather.gatherCodeNativeNotebook",
          "title": "Gather code",
          "when": "false"
        },
        {
          "command": "gather.quality",
          "title": "Gather.quality",
          "when": "false"
        }
      ],
      "notebook/cell/title": [
        {
          "command": "gather.gatherCodeNativeNotebook",
          "when": "notebookViewType == jupyter-notebook && notebookCellType == code && notebookCellRunState == 3",
          "group": "inline/cell@2"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Gather",
      "properties": {
        "gather.gatherToScript": {
          "type": "boolean",
          "default": false,
          "description": "Gather code to a python script rather than a notebook.",
          "scope": "resource"
        },
        "jupyter.gatherSpecPath": {
          "type": "string",
          "default": "",
          "description": "This setting specifies a folder that contains additional or replacement spec files used for analysis.",
          "scope": "resource"
        }
      }
    }
  },
  "extensionDependencies": [
    "ms-toolsai.jupyter"
  ],
  "scripts": {
    "prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-watch": "webpack --mode development --watch",
    "lint": "eslint src --ext ts",
    "compile": "tsc -watch -p ./",
    "vsix": "vsce package",
    "npm-auth": "vsts-npm-auth -config .npmrc",
    "test": "mocha --config ./src/test/.mocha.config.json"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.1",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/stack-trace": "0.0.29",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.2.0",
    "filemanager-webpack-plugin-fixed": "^2.0.9",
    "glob": "^7.1.6",
    "mocha": "^7.2.0",
    "monaco-editor": "0.18.1",
    "terser-webpack-plugin": "^3.0.6",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typemoq": "^2.1.0",
    "typescript": "^4.0.2",
    "vscode-test": "^1.3.0",
    "webpack": "^5.0.0-alpha.13",
    "webpack-cli": "^3.3.12",
    "webpack-obfuscator": "^1.6.0"
  },
  "dependencies": {
    "@jupyterlab/coreutils": "^3.1.0",
    "@jupyterlab/services": "^4.2.0",
    "@msrvida/python-program-analysis": "^0.4.45",
    "@opentelemetry/api": "^0.11.0",
    "@opentelemetry/tracing": "^0.11.0",
    "@types/uuid": "^3.4.3",
    "fs-extra": "^4.0.3",
    "npm": "^6.14.8",
    "path": "^0.12.7",
    "stack-trace": "0.0.10",
    "uuid": "^3.3.2",
    "vscode-extension-telemetry": "0.1.4",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
